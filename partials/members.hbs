{{#if categories}}
  {{#each categories}}
    {{#unless allChildrenHaveOwnDocument}}
      <h3 class="subsection-title">{{title}}</h3>
      {{#each children}}
        {{#unless hasOwnDocument}}
          {{> member}}
        {{/unless}}
      {{/each}}
    {{/unless}}
  {{/each}}
{{else}}
  {{#each groups}}
    {{#unless allChildrenHaveOwnDocument}}
      {{> members.group}}
    {{/unless}}
  {{/each}}
{{/if}}
