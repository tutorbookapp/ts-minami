<div class="section-method">
  <a name="{{anchor}}" class="tsd-anchor"></a>
  {{#if name}}
    <h4 class="name">
      {{#each flags}}
        <span class="type-signature">({{this}}) </span>
      {{/each}}<!--
      -->{{name}}<!--
      -->{{#each signatures}}
        <span class="signature">(
          {{#each parameters}}
            {{#if @index}}, {{/if}}
            {{#if flags.isRest}}
              <span class="signature-attributes">...</span>
            {{/if}}
            {{name}}
            <span class="signature-attributes">
              {{#if flags.isOptional}}opt{{/if}}
              {{#if defaultValue}}opt{{/if}}
            </span>
          {{/each}}<!--
        -->)</span>
        {{#if type}}
          <span class="type-signature"> â†’ <span>{</span>{{#with type}}{{>type}}{{/with}}<span>}</span></span>
        {{/if}}
      {{/each}}
    </h4>
  {{/if}}

  {{#if signatures}}
    {{> member.signatures}}
  {{else}}{{#if hasGetterOrSetter}}
    {{> member.getterSetter}}
  {{else}}{{#if isReference}}
    {{> member.reference}}
  {{else}}
    {{> member.declaration}}
  {{/if}}{{/if}}{{/if}}

  {{#each groups}}
    {{#each children}}
      {{#unless hasOwnDocument}}
        {{> member}}
      {{/unless}}
    {{/each}}
  {{/each}}
</div>
